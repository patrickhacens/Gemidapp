<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Gemidapp.Views.TokenPage"
             Title="{Binding Title}">
    <StackLayout Spacing="20" Padding="15">
        <Label Text="Token" FontSize="Medium" />
        <Entry Text="{Binding Token}" FontSize="Small" />
        <Label Text="Não possui um token?" FontSize="Medium" />
        <Button Text="Click aqui parar adquirir um token" Command="{Binding OpenWebBrowser}"/>
    </StackLayout>
</ContentPage>